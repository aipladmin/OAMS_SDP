Experience_Reports.html
{% extends "index.html" %}
{% block content %}
<style>
	.feather-24{
		width: 24px;
		height: 24px;
	}
	@media print {
		@page
		{
			size: A4 portrait;
		}
		body * {
			visibility: hidden;

		}
		.prnt * {
			visibility: hidden;
		}
		#cntnr * {
			visibility: visible;
			position: relative;	
		}

	}
</style>

{{ data }} 

<div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-5 pb-2 mb-3  border-bottom">
	<h1 class="h2"><center>Summary Reports  </center></h1>
	<div class="btn-toolbar mb-2 mb-md-0">
	</div>

</div>

<div class="container" id="cntnr">
	<h3 style="text-align: center;">Experience Wise Reports</h3>
	<p>
		
	</p>
	
	<button class="feather-24" data-feather="printer" onclick="window.print();return false;" value="print" id="prnt" style="float:right;">
	</button>
	<table class="table">
		<thead class="thead-dark">
			<tr>
				<th>Full Name</th>
				<th> Email ID</th>
				<th>Experience</th>
				{# <th>Profession</th> #}
				<!-- <th>Profession</th> -->
			</tr>
		</thead>
		<tbody>
			{% for item in data %}				
			<tr>
				<td>{{ item[0] }}</td>
				<td>{{ item[1] }}</td>
				<td>{{ item[2] }}</td>
				{# <td>{{ item['Profession'] }}</td> #}
				
			</tr>
			{% endfor %} 
		</tbody>
	</table>

	
</div>
{% endblock %}	